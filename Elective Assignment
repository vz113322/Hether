	<form>
		<input type="text"  id="put1" onkeyup="write1()"><br /></input>
		<input type="text" 	id="put2" onkeyup="write2()"><br /></input>
		<input type="text" 	id="put3" onkeyup="write3()"><br /></input>
		<input type="text"  id="put4" onkeyup="write4()"><br /></input>
		<input type="text"  id="put5" onkeyup="write5()"><br /></input>
